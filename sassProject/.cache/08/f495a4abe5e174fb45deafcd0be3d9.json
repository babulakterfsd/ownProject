    width: 100%;\r\n        height: 120px;\r\n      }\r\n    }\r\n    .navber--content {\r\n      width: 60%;\r\n      display: flex;\r\n      align-items: flex-start;\r\n      justify-content: space-between;\r\n      margin-top: 2rem;\r\n      margin-right: 0.8rem;\r\n      .navber--content--item {\r\n        list-style: none;\r\n        a,\r\n        a:link,\r\n        a:active {\r\n          text-decoration: none;\r\n          padding: 4px;\r\n          color: rgb(14, 35, 83);\r\n          font-size: 20px;\r\n          font-weight: 500;\r\n          &:hover {\r\n            color: white;\r\n            background-color: rgb(14, 24, 47);\r\n          }\r\n        }\r\n      }\r\n      .inputbox {\r\n        .input {\r\n          padding: 10px 1rem;\r\n          border: 2px solid silver;\r\n          font-size: 1rem;\r\n          color: grey;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .heading1 {\r\n    color: $font-color;\r\n    font-size: 4vw;\r\n    letter-spacing: 2px;\r\n    font-weight: 300;\r\n    margin: 3rem 0 1rem 0;\r\n  }\r\n}\r\n//  End of header style\r\n\r\n// awesome--section style\r\n.awesome--section {\r\n  display: flex;\r\n  justify-content: space-around;\r\n  flex-shrink: 1;\r\n  //   flex-wrap: wrap;\r\n  .awesome--section--content {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    width: 50%;\r\n    font-size: 2rem;\r\n    padding: 0.625rem;\r\n    color: rgba(107, 113, 127, 0.6);\r\n    font-weight: 300;\r\n  }\r\n  .awesome--section--logo {\r\n    padding: 0.625rem;\r\n    width: 50%;\r\n    .glass {\r\n      width: 100%;\r\n    }\r\n  }\r\n}\r\n\r\n// info class style ;\r\n.info {\r\n  background-color: $font-color;\r\n  display: flex;\r\n  justify-content: center;\r\n  color: white;\r\n  list-style: none;\r\n  padding: 0.9375rem 0 1.5625rem 0;\r\n  .link {\r\n    padding: 0 0.625rem;\r\n    font-weight: 500;\r\n  }\r\n  a,\r\n  a:link,\r\n  a:active {\r\n    color: white;\r\n    padding: 0.1875rem;\r\n  }\r\n  a:hover {\r\n    text-decoration: none;\r\n    background-color: rgb(137, 123, 147);\r\n  }\r\n}\r\n\r\n//  Gird section style here :\r\n.grid-section {\r\n  padding: 3.125rem;\r\n  margin-top: 1.25rem;\r\n  display: grid;\r\n  grid-template-rows: repeat(2, auto);\r\n  grid-template-columns: repeat(3, 1fr);\r\n  grid-gap: 1.875rem;\r\n  .box {\r\n    // border: 2px solid blue;\r\n    padding: 0.625rem;\r\n    h2 {\r\n      // text-align: center;\r\n      margin-bottom: 1.25rem;\r\n    }\r\n  }\r\n}\r\n//  footer style here:\r\n.footer {\r\n  background-color: rgb(248, 247, 247);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  // flex-shrink: 1;\r\n  flex-wrap: wrap;\r\n  padding: 1.875rem;\r\n  .footer--content {\r\n    margin: 1.875rem 0;\r\n    .text {\r\n      max-width: 1000px;\r\n    }\r\n    .linklist {\r\n      margin-top: 1.25remx;\r\n      display: flex;\r\n      list-style: none;\r\n      a,\r\n      a:link,\r\n      a:active {\r\n        border: 2px solid white;\r\n        color: black;\r\n        text-decoration: none;\r\n        padding: 0.3125rem;\r\n        margin: 0.3125rem;\r\n        &:hover {\r\n          background-color: rgb(228, 227, 240);\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .btn {\r\n    margin-right: 3.125rem;\r\n    a,\r\n    a:link,\r\n    a:active {\r\n      padding: 0.3125rem 0.625rem;\r\n      text-decoration: none;\r\n      color: white;\r\n      background-color: rgb(136, 111, 226);\r\n      border: none;\r\n      border-radius: 0.625rem;\r\n      &:hover {\r\n        background-color: rgb(112, 89, 199);\r\n      }\r\n    }\r\n  }\r\n}\r\n.hidden {\r\n  display: none !important;\r\n}\r\n// documentaion page style::\r\n\r\n.documentation {\r\n  padding: 10px;\r\n  border: 2px solid blue;\r\n  display: flex;\r\n  align-items: center;\r\n  .documentation--content {\r\n    border: 2px solid blue;\r\n    width: 50%;\r\n    margin-right: 1rem;\r\n    li {\r\n      margin: 1rem 5px;\r\n      list-style: none;\r\n    }\r\n    a,\r\n    a:link,\r\n    a:active {\r\n      text-decoration: none;\r\n      padding: 5px 10px;\r\n      border: 1px solid silver;\r\n      font-size: 1.2rem;\r\n    }\r\n  }\r\n  .documentation--information {\r\n    border: 2px solid yellow;\r\n    width: 70%;\r\n    .mutetext {\r\n      display: flex;\r\n      justify-content: center;\r\n      align-items: center;\r\n      text-align: justify;\r\n      font-size: 2rem;\r\n      padding: 0.625rem;\r\n      color: rgba(107, 113, 127, 0.6);\r\n      font-weight: 300;\r\n      margin-bottom: 1rem;\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"187ddcdd56292b56ed14393da54bd8dc","cacheData":{"env":{}}}